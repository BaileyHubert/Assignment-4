package main;

public class Question3 {

	public static void main(String[] args) {
		// create variable a and set it to 0
		int a = 0;
		//create for loop
		for(int b = 1; b<=10; b++) {
			//adds value b to variable a and assigns the new value to variable a
			a += b;
		}
		//prints resulting value
		System.out.println(a);
	}

}
